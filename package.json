  "license": "MIT",
  "dependencies": {
    "@nieuwlandgeo/sldreader": "^0.2.16",
    "geostyler-mapfile-parser": "^2.0.0",
    "geostyler-sld-parser": "^5.0.0",
    "geostyler-style": "^7.2.0",
    "ol": "^7.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.13"
  }
}
