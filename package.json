{
  "name": "sld2mapfile",
  "version": "1.0.0",
  "description": "The converter SLD file to Mapfile",
  "bin": {
    "sld2map": "./bin/index.js"
  },
  "scripts": {
    "compile": "npx tsc bin/index.ts",
    "run": "npx ts-node bin/index.ts",
    "test": "npx tsc bin/index.ts && node bin/index.js --sld test.sld --table test",
    "deploy": "npx tsc bin/index.ts && npm link"
  },
